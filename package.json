{
  "name": "mongodb-bk",
  "version": "1.3.0",
  "description": "A library for backing up and restoring MongoDB to Google Drive",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/tests/**/*.test.ts"
    ],
    "collectCoverage": true
  },
  "keywords": [
    "mongodb",
    "backup",
    "restore",
    "google drive",
    "database"
  ],
  "author": "Guilherme Lima (https://github.com/guilhermelim)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/guilhermelim/mongodb-bk.git"
  },
  "bugs": {
    "url": "https://github.com/guilhermelim/mongodb-bk/issues"
  },
  "homepage": "https://github.com/guilhermelim/mongodb-bk#readme",
  "dependencies": {
    "@googleapis/docs": "^3.0.0",
    "bson": "^6.3.0",
    "googleapis": "^133.0.0",
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.11.17",
    "dotenv": "^16.4.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
}
